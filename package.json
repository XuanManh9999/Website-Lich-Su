{
  "name": "website-lich-su",
  "version": "1.0.0",
  "description": "Website giới thiệu nhân vật lịch sử Việt Nam",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && nodemon index.js",
    "client": "cd client && npm start",
    "install-server": "cd server && npm install",
    "install-client": "cd client && npm install",
    "install-all": "npm install && npm run install-server && npm run install-client",
    "seed": "cd server && node scripts/seedData.js",
    "create-tables": "cd server && node scripts/createTables.js"
  },
  "keywords": ["history", "vietnam", "nfc", "audio"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
